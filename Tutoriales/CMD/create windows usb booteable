WINDOWS

	diskpart

	-->
		list disk
		select disk #
		detail disk
		clean

		create partition primary
		active
		format fs=fat32

		assign
		exit
	<--	

	cd ..
	cd ..
	cd windows\system32\
		--> 
		xcopy D:\*.* /s/e/f E:\


		
